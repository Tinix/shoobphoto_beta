<div class="row">
<div class="col-sm-6">
<%= link_to "Upload CSV", admin_csv_students_path, class: "btn btn-primary" %>

 </div>
 <div class="col-sm-6">
 <%= form_tag admin_where_empty_students_path, multipart: true do %>
  <%= select_tag :school, options_from_collection_for_select(School.all, "id", "name")
 %>
  <%= submit_tag "Find Missing Images by School", class: "btn btn-primary" %>
<% end %>
</div>
</div>
<br />
<table class="table table-striped">
	<thead>
		<th>
			First Name
		</th>
		<th>
			Last Name
		</th>
		<th>
			Student ID
		</th>
		<th>
			School
		</th>
	</thead>
	<tbody>
<% @students.each do |student| %>
	<tr>
		<td>
			<%= student.first_name %>
		</td>
		<td>
			<%= student.last_name %>
		</td>
		<td>
			<%= student.student_id %>
		</td>
		<td>
		<%= student.school.try(:name) %>
		</td>
	</tr>
<% end %>
</tbody>
</table>

<%= will_paginate %>