<%= simple_form_for([:admin, @addon]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
  	<%= f.input :name %>
  	<%= f.input :price_with %>
  	<%= f.input :price_without %>
  	<%= f.input :image_count %>
  	<%= f.input :css_name %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
