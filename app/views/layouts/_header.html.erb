<div align="center">
<%= link_to image_tag("logo.jpg", :class => 'header'), root_path %> 
</div>
<div id="navcontainer">
<ul>
      <% @nav_links.each do |nav_link| %>
        <li<%= request.fullpath.split('/')[1] == nav_link.slug ? " class=current" : '' %>>
          <%= link_to nav_link.title, nav_link.path %>
        </li>
      <% end %>
      </ul>
</div>
<br />