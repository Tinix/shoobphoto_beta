<% @students.each do |student| %>
       <tr id="student_<%= student.id %>">
          <td id="button_<%=student.id %>">
            <%= link_to "Remove", export_list_remove_path(student.id), remote: true, class: "btn btn-danger" %>
          </td>
          <td>
            <%= student.student_id %>
          </td>
           <td>
            <%= student.first_name %>
          </td>
           <td>
            <%= student.last_name %>
          </td>
           <td>
            <%= student.grade %>
          </td>
           <td>
            <%= student.teacher %>
          </td>
          <td>

            <%= student.user_students.where(:user_id => current_user.id).map{|x| x.award.try(:abbreviation)}.join(", ") %>
          </td>
      </tr>
<% end %>