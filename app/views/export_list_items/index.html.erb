<h1>Listing export_list_items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @export_list_items.each do |export_list_item| %>
      <tr>
        <td><%= link_to 'Show', export_list_item %></td>
        <td><%= link_to 'Edit', edit_export_list_item_path(export_list_item) %></td>
        <td><%= link_to 'Destroy', export_list_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Export list item', new_export_list_item_path %>
