$('#myModal').modal('hide');
<% @ids.each do |id| %>
<% @student = Student.find(id) %>
    $("#student_<%= id %>_check").html("<%= escape_javascript(render 'remove_student', :locals => {:student => @student }) %>");
<% end %>

$("#select_all").html("<%= escape_javascript(render 'deselect_all') %>");

