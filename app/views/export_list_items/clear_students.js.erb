$('#myModal').modal('hide');
<% @export_list.awards.each do |award| %>
<% @ids.each do |id| %>

<% @student = Student.find(id) %> 
    $("#student_<%= id %>_<%= award.id %>_check").html("<%= escape_javascript(render 'remove_student', :locals => {:student => @student, :award => award }) %>");
<% end %>
<% end %>
$("#select_all").html("<%= escape_javascript(render 'deselect_all') %>");

 