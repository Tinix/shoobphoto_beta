<% if @award.nil? %>
$("#student_<%= @student.id %>_check").html("<%= escape_javascript(render 'remove_student', :locals => {:student => @student}) %>");
<% else %>
$("#student_<%= @student.id %>_<%= @award.to_i %>_check").html("<%= escape_javascript(render 'remove_student', :locals => {:student => @student}) %>");

<% end %>