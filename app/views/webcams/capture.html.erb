<%= headshot_default_config(Rails.application.config.session_options[:key], headshot_capture_path) %>

<%= headshot_camera %>

<br/><br/>

<%= headshot_snap_button('Capture',id: "capture")  %>


<script>
$("#capture").click(function() {
  $.ajax({
      url: "/webcams/newstudent?package_id=<%= params[:id] %>",
      type: "GET",
      success: function (data) { 
          // append data to your page
          $("page_element").html(data);
      }
  });
});
</script>
