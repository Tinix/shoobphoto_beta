<%= simple_form_for(@school_note) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :cdscode %>
    <%= f.input :name %>
    <%= f.input :district_id %>
    <%= f.input :city_id %>
    <%= f.input :address %>
    <%= f.input :phone %>
    <%= f.input :principle %>
    <%= f.input :secretary %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
