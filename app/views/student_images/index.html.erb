<h1>Listing student_images</h1>

<table>
  <thead>
    <tr>
      <th>Package</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_images.each do |student_image| %>
      <tr>
        <td><%= student_image.package_id %></td>
        <td><%= student_image.student_id %></td>
        <td><%= link_to 'Show', student_image %></td>
        <td><%= link_to 'Edit', edit_student_image_path(student_image) %></td>
        <td><%= link_to 'Destroy', student_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student image', new_student_image_path %>
